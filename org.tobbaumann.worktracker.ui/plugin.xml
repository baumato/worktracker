<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="WorkTracker">
         <property
               name="appName"
               value="WorkTracker">
         </property>
         <property
               name="applicationXMI"
               value="org.tobbaumann.worktracker.ui/Application.e4xmi">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/org.tobbaumann.worktracker.ui/css/default.css">
         </property>
         <property
               name="aboutImage"
               value="icons/clock_128x128.png">
         </property>
         <property
               name="aboutText"
               value="Copyright (c) 2013 Tobias Baumann.&#x0A;All rights reserved. This program and the accompanying materials&#x0A;are made available under the terms of the Eclipse Public License v1.0&#x0A;which accompanies this distribution, and is available at&#x0A;http://www.eclipse.org/legal/epl-v10.html&#x0A;&#x0A;All clock icons are from http://www.icons-land.com.">
         </property>
         <property
               name="windowImages"
               value="icons/clock_16x16.png,icons/clock_32x32.png,icons/clock_48x48.png,icons/clock_64x64.png,icons/clock_128x128.png">
         </property>
      </product>
   </extension>

</plugin>
